<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:top="-1dp"
        android:left="-1dp"
        android:right="-1dp">
        <shape android:shape="rectangle" >
            <solid android:color="@color/white" />

            <stroke
                android:width="1dp"
                android:color="#EFEFEF" />
            <padding android:top="@dimen/margin_md"
                android:bottom="@dimen/margin_md"
                android:left="@dimen/margin_md"
                android:right="@dimen/margin_md"/>
        </shape>
    </item>

</layer-list>
